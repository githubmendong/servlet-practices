    </servlet>
    <servlet-mapping>
        <servlet-name>_01Servlet</servlet-name>
        <url-pattern>/01</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>_02Servlet</display-name>
        <servlet-name>_02Servlet</servlet-name>
        <servlet-class>servlets._02Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>_02Servlet</servlet-name>
        <url-pattern>/02</url-pattern>
    </servlet-mapping>
</web-app>